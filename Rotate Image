//not in place version

class Solution {
public:
    void rotate(vector<vector<int> > &matrix) {
        vector<vector<int>> ret;
        int len=matrix.size();
        for(int i=0;i<len;++i){
            vector<int> temp;
            for(int j=len-1;j>=0;--j){
                temp.push_back(matrix[j][i]);
            }
            ret.push_back(temp);
        }
        matrix=ret;
    }
};
